{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1",
    "FLASK_ENV": "production",
    "FLASK_DEBUG": "0"
  },
  "regions": ["hkg1"]
} 