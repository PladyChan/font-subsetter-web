{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "functions": {
    "api/*.py": {
      "maxDuration": 60,
      "memory": 3008
    }
  },
  "build": {
    "env": {
      "MAX_CONTENT_LENGTH": "104857600"
    }
  }
} 